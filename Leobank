select q.id_operation,id_point,account,time_server,time_point,cast(sum_income as numeric)/100 as sum_income,
cast(sum_outcome as numeric)/100 sum_outcome,cast(sum_comm as numeric)/100 sum_comm  from operations.master q where id_service =959 
and  q.state = 60    
       and q.substate = 0
        and q.time_process >= '2022-05-25' 
       and q.time_process < '2022-06-01' 
       and q.time_server >= date '2022-05-25' - interval '7day'
       and q.time_server < date '2022-06-01' + interval '7day'
