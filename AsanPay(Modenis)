
Select cast(StatusDate as date),ProviderName,s.ServiceName,agentterminalid,count(*) 'Say' from [dbo].[Payment] p with (nolock)
join [dbo].[Service] s with (nolock)  on p.ServiceID=s.ServiceID
join [dbo].[Provider] pp with (nolock) on s.ProviderID=pp.ProviderID
where StatusDate between '2022-03-01' and '2022-04-29' and p.ServiceID in (1072,1073,1074,1075,1076,828,829,748,749,750,632,649,1066,1071,1014,
1015,1099,1114,1115,877,878,965,883,881,882,879,880,631,672,680,689,698,704,705,706,707,708,709,
710,711,712,726,727,747,756,757,758,759,1035,1193,1194,884,1000,751,752,753,754,755,760,764)
group by cast(StatusDate as date),ProviderName,s.ServiceName,agentterminalid
